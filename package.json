{
  "name": "app",
  "version": "1.0.0",
  "type": "module",
  "main": "backend/api/index.js",
  "scripts": {
    "start": "node backend/server.js",
    "start:debug": "node --inspect --max-old-space-size=8192 backend/server.js",
    "dev": "nodemon backend/server.js",
    "dev:debug": "nodemon --inspect --max-old-space-size=8192 backend/server.js",
    "frontend": "http-server frontend -p 5500 -c-1",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend\"",
    "dev:css": "npx tailwindcss -i ./frontend/styles/input.css -o ./frontend/styles/output.css --watch",
    "build": "npx tailwindcss -i ./frontend/styles/input.css -o ./frontend/styles/output.css --minify",
    "seed": "node backend/utils/seedPlaces.js",
    "seed:prod": "node -r dotenv/config backend/utils/seedPlaces.js dotenv_config_path=backend/.env.production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "node-fetch": "^3.3.2",
    "pexels": "^1.4.0",
    "unsplash-js": "^7.0.20",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "daisyui": "^5.5.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.11"
  },
  "engines": {
    "node": ">=18.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanzsn/kipifyApp.git"
  },
  "bugs": {
    "url": "https://github.com/hanzsn/kipifyApp/issues"
  },
  "homepage": "https://github.com/hanzsn/kipifyApp#readme",
  "description": ""
}
